{
  "name": "test-interface-codemod",
  "version": "1.0.0",
  "description": "A collection of codemods for test interface transformation",
  "author": {
    "name": "Damian Krzeminski",
    "email": "pirxpilot@furkot.com",
    "url": "https://pirxpilot.me"
  },
  "repository": "pirxpilot/test-interface-codemod",
